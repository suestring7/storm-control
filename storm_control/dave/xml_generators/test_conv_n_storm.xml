<?xml version="1.0" encoding="ISO-8859-1"?>
<recipe>
  <command_sequence>
    <change_directory>D:\default\test_dave</change_directory>
    <loop name = "Movie Loop" increment = "name"> 
      <item name = "Conv"></item>
      <item name = "Bleach"></item>
      <item name = "Storm"></item>
    </loop>
  </command_sequence>

  <item name="Conv">
    <movie>
      <name increment="Yes">conv</name>
      <delay>0</delay>
      <length type="int">20</length>
      <parameters type="str">10HZ_256x256_488</parameters>
      <overwrite type="bool">False</overwrite>
      <variable_entry name = "Movie Loop"></variable_entry>
    </movie>
  </item>
  <item name="Bleach">
    <movie>
      <name increment="Yes">bleach</name>
      <length type="int">20</length>
      <parameters type="str">110HZ_256x256_AF647</parameters>
      <overwrite type="bool">False</overwrite>
      <variable_entry name = "Movie Loop"></variable_entry>
    </movie>
    </item>
  <item name="Storm">
    <movie>
      <name increment="Yes">storm</name>
      <length type="int">1000</length>
      <parameters type="str">110HZ_256x256_AF647</parameters>
      <overwrite type="bool">False</overwrite>
      <variable_entry name = "Movie Loop"></variable_entry>
    </movie>
  </item>
  <loop_variable name = "Movie Loop">
     <file_path></file_path>
  </loop_variable>
</recipe>
